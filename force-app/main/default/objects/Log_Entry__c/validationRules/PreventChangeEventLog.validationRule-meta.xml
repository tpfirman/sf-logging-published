<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PreventChangeEventLog</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISCHANGED(Event_Log__c), NOT(ISNEW()))</errorConditionFormula>
    <errorDisplayField>Event_Log__c</errorDisplayField>
    <errorMessage>Event Log value cannot be changed after record is created.</errorMessage>
</ValidationRule>
